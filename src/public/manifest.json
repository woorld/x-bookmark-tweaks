{
  "name": "X Bookmark Tweaks",
  "version": "1.0.0",
  "description": "Xのブックマークを調整・拡張する機能群です。",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "512": "img/icon/icon512.png"
  }
}
